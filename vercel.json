{
  "version": 2,
  "builds": [
    {
      "src": "Blogsite/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/python"  // Change to @vercel/python
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Blogsite/wsgi.py"
    }
  ]
}
